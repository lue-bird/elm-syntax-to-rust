rustExpressionPrependStatements :
    List RustStatement
    -> RustExpression
    -> RustExpression
rustExpressionPrependStatements statements result =
    statements
        |> List.foldr
            (\statement resultSoFar ->
                RustExpressionAfterStatement { statement = statement, result = resultSoFar }
            )
            result


