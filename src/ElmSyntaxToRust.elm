pub fn array_push<'a, A: Clone>(
    allocator: &'a Bump,
    new_last_element: A,
    array: &ArrayArray<A>,
) -> &'a ArrayArray<A> {
    let mut array_as_vec: Vec<A> = array.to_vec();
    array_as_vec.push(new_last_element);
    allocator.alloc(array_as_vec)
