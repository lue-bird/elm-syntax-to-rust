                switch endianness {
                case .Bytes_BE:
                    bytesBuffer.append(contentsOf: toBytes(f64.bitPattern.bigEndian))
                case .Bytes_LE:
                    bytesBuffer.append(contentsOf: toBytes(f64.bitPattern.littleEndian))
