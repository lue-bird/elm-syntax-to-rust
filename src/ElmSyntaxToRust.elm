// For an index where -1 meaning one before the last element, 1 meaning one after the first element,
// normalize to valid index from the start
static func possiblyNegativeIndexForCount(index: Int, count: Int) -> Int {
    if index >= 0 {
        min(index, count)
    } else {
        max(count + index, 0)
    }
}

