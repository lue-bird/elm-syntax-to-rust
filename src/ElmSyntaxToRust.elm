        RustStatementSwitch switch ->
            (switch.matched
                |> rustExpressionCountUsesOfReference referenceToCountUsesOf
            )
                + (switch.case0.statements
                    |> listMapAndSum
                        (\statement ->
                            statement |> rustStatementCountUsesOfReference referenceToCountUsesOf
                        )
                  )
                + (switch.case1Up
                    |> listMapAndSum
