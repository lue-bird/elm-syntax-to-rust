pub fn basics_mod_by(by: f64, base: f64) -> f64 {
    // https://github.com/elm/core/blob/1.0.5/src/Elm/Kernel/Basics.js#L20
    // https://www.microsoft.com/en-us/research/wp-content/uploads/2016/02/divmodnote-letter.pdf
    if by == 0_f64 {
        panic!("mod by 0")
