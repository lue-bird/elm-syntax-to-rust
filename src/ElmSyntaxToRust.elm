            printRustExpressionNotParenthesized
                rustResultExpression
    in
    printExactlyReturn
        |> Print.followedBy
            (Print.withIndentAtNextMultipleOf4
                (Print.spaceOrLinebreakIndented
                    (rustResultExpressionPrint |> Print.lineSpread)
                    |> Print.followedBy
                        rustResultExpressionPrint
