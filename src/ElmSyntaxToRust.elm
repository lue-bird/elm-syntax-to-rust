        ElmSyntaxTypeInfer.ExpressionReferenceVariant reference ->
            let
                asBool : Maybe RustExpression
                asBool =
                    case reference.moduleOrigin of
                        "Basics" ->
                            case reference.name of
                                "True" ->
                                    Just
                                        (RustExpressionReference
                                            { moduleOrigin = Nothing
                                            , name = "true"
