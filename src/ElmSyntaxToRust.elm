static func typeErasedEq<a: Equatable, b: Equatable>(_ a: a, _ b: b) -> Bool {
    if let b: a = b as? a {
        a == b
    } else {
        fatalError("== on non-Equatable types")
    }
}
static func typeErasedNeq<a: Equatable, b: Equatable>(_ a: a, _ b: b) -> Bool {
    if let b: a = b as? a {
        a != b
    } else {
        fatalError("/= on non-Equatable types")
    }
}

